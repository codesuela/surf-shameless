{
    "name": "Embarrassment Filter",
    "version": "0.8",
    "manifest_version": 2,
    "description": "This extension saves you from embarrassment",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "32": "icon.png"
    },
    "permissions": [ "tabs", "http://*/*", "https://*/*", "history", "contextMenus"],
    "background":{
        "scripts": ["js/aes.js", "build/background_clean.js"]
    },
    "options_page": "settings.html"
}