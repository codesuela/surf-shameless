{
    "name": "Embarrassment Filter",
    "version": "0.0.1",
    "manifest_version": 1,
    "description": "This extension saves you from embarrassment",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [ "tabs", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "history" ],
    "background_page": "background.html",
    "options_page": "settings.html"
}