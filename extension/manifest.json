{
    "name": "Embarrassment Filter",
    "version": "0.0.1",
    "manifest_version": 1,
    "description": "This extension saves you from embarrassment",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [ "tabs", "http://*/*", "webRequest", "webRequestBlocking", "history" ],
    "background_page": "background.html"
}